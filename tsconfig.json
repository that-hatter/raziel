{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "lib": ["ES2017", "DOM"],
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "outDir": "./dist"
  },
  "compileOnSave": true
}
